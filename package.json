{
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "electron": "^23.1.2",
    "electron-packager": "^17.1.1"
  },
  "name": "electron-project",
  "version": "1.0.0",
  "description": "Electron template app",
  "main": "main.js",
  "icon": "icon.ico",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --platform=win32 --arch=x64 --icon=icon.ico --out=release-builds --overwrite --asar --prune --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron project\""
  },
